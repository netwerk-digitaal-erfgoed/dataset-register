{
  "@context": {
    "dcat": "http://www.w3.org/ns/dcat#",
    "dct": "http://purl.org/dc/terms/",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "sh": "http://www.w3.org/ns/shacl#"
  },
  "@graph": [
    {
      "@type": "sh:NodeShape",
      "sh:targetClass": {
        "@id": "dcat:Dataset"
      },
      "sh:property": [
        {
          "sh:path": {
            "@id": "dct:title"
          },
          "sh:minCount": 1
        },
        {
          "sh:path": {
            "@id": "dct:alternative"
          },
          "sh:minCount": 0
        },
        {
          "sh:path": {
            "@id": "dct:description"
          },
          "sh:minCount": 1
        },
        {
          "sh:path": {
            "@id": "dct:license"
          },
          "sh:minCount": 1
        },
        {
          "sh:path": {
            "@id": "dct:creator"
          },
          "sh:minCount": 1,
          "sh:class": {
            "@id": "foaf:Organization"
          },
          "sh:node": "dcat:CreatorShape"
        },
        {
          "sh:path": {
            "@id": "dcat:distribution"
          },
          "sh:minCount": 1,
          "sh:class": {
            "@id": "dcat:Distribution"
          },
          "sh:node": "dcat:DistributionShape"
        }
      ]
    },
    {
      "@id": "dcat:CreatorShape",
      "@type": "sh:NodeShape",
      "sh:targetClass": {
        "@id": "foaf:Organization"
      },
      "sh:property": [
        {
          "sh:path": {
            "@id": "foaf:name"
          },
          "sh:minCount": 1
        }
      ]
    },
    {
      "@id": "dcat:DistributionShape",
      "@type": "sh:NodeShape",
      "sh:targetClass": {
        "@id": "dcat:Distribution"
      },
      "sh:property": [
        {
          "sh:path": {
            "@id": "dcat:accessURL"
          },
          "sh:minCount": 1
        },
        {
          "sh:path": {
            "@id": "dct:format"
          },
          "sh:minCount": 1
        }
      ]
    }
  ]
}
