{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "outDir": "build/",
    "sourceMap": true,
    "target": "es2018",
    "lib": [
      "ES2020"
    ],
    "module": "es2022",
    "declarationMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "declaration": true
  },
  "include": [
    "src/"
  ]
}
